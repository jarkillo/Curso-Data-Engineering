# AWS Configuration
# IMPORTANTE: Este archivo es un TEMPLATE. Copia como .env y rellena con tus valores reales.
# NUNCA commits el archivo .env a Git

# AWS Credentials (obtener de AWS Console → IAM → Security credentials)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_DEFAULT_REGION=us-east-1

# S3 Buckets
S3_BUCKET_RAW=cloudapi-logs-raw
S3_BUCKET_PROCESSED=cloudapi-logs-processed

# Lambda Configuration
LAMBDA_FUNCTION_NAME=ValidarLogsAPI
LAMBDA_TIMEOUT=60
LAMBDA_MEMORY_MB=512

# Glue Configuration
GLUE_DATABASE=cloudapi_analytics
GLUE_TABLE_RAW=logs_raw
GLUE_TABLE_PROCESSED=logs_processed
GLUE_JOB_NAME=TransformLogsETL

# Athena Configuration
ATHENA_OUTPUT_LOCATION=s3://cloudapi-logs-processed/athena-results/
ATHENA_WORKGROUP=primary

# Logging
LOG_LEVEL=INFO
